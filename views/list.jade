extends layout

block content
  h1 Super Hero Log
  a(href="/") home
  div(class="row")
    div(class="col-md-6 col-md-offset=3")
      h2 View Heros
      each hero in heros
        table 
          tr
            td
              a(href="/list/#{hero._id}")= hero.hero
            td= hero.power
            
